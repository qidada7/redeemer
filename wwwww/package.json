{ "name": "RYKG",
  "productName": "三维重建图像处理",
  "description": "An advanced viewer to render DICOM files and 3D model files with full of functions and tools",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "DataminTech.com",
    "email": "Admin@DataminTech.com",
    "url": "http://datamintech.com/"
  },
  "copyright": "© 2019, DataminTech Pty. Ltd.",
  "homepage": "http://datamintech.com/",
  "main": "app/background.js",
  "build": {
    "artifactName": "${name}-${version}-${os}-${arch}.${ext}",
    "productName": "三维重建图像处理",
    "appId": "com.datamintech.amdv",
    "files": [
      "app/**/*",
      "!app/data/",
      "node_modules/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": null
  },
  "scripts": {
    "build": "webpack --config=build/webpack.app.config.js --env=production",
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test --display=none && webpack --config=build/webpack.e2e.config.js --env=test --display=none",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "release": "npm test && yarn build && electron-builder",
    "package-win": "yarn build && electron-builder build --win --x64 --publish never",
    "version": "echo $npm_package_version"
  },
  "dependencies": {
    "@garycourt/node-machine-id": "^1.1.12",
    "ami.js": "0.23.0",
    "dat.gui": "0.7.3",
    "dexie": "2.0.4",
    "directory-exists": "^2.0.1",
    "draggabilly": "^2.2.0",
    "electron-log": "2.2.17",
    "electron-unhandled": "^2.1.0",
    "electron-updater": "^4.0.5",
    "file-exists": "^5.0.1",
    "fs-jetpack": "^2.1.0",
    "jquery": "^3.3.1",
    "jquery-ui-bundle": "^1.12.1-migrate",
    "knockout": "^3.4.2",
    "license-key-gen": "^0.1.2",
    "node-stl": "^0.7.0",
    "noty": "^3.2.0-beta",
    "source-code-pro": "2.30.1",
    "stl-loader": "1.0.0",
    "string-format": "^2.0.0",
    "three": "0.99.0",
    "three-orbitcontrols": "^2.99.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "chai": "^4.1.0",
    "css-loader": "^1.0.0",
    "electron": "3.0.10",
    "electron-builder": "20.38.2",
    "electron-mocha": "^6.0.4",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.0",
    "mocha": "^5.2.0",
    "source-map-support": "^0.5.6",
    "spectron": "^3.8.0",
    "style-loader": "^0.21.0",
    "terser-webpack-plugin": "^1.2.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">=10.x ",
    "yarn": ">=1.x"
  },
  "browserslist": "electron 1.6"
}
